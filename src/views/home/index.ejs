  <script>
  document.addEventListener('DOMContentLoaded', (event) => {
    const swipeTrack = document.getElementById('swipeTrack');
    const indicators = document.getElementById('swipeIndicator').children;
    const items = document.querySelectorAll('.van-swipe-item');
    let currentIndex = 0;

    function updateIndicator(index) {
      for (let i = 0; i < indicators.length; i++) {
        indicators[i].classList.remove('active');
      }
      indicators[index].classList.add('active');
    }

    function swipe() {
      currentIndex = (currentIndex + 1) % items.length;
      swipeTrack.style.transform = `translateX(-${currentIndex * 100}%)`;
      updateIndicator(currentIndex);
    }

    updateIndicator(currentIndex);

    setInterval(swipe, 2500); // Swipe every half second
  });
</script>
